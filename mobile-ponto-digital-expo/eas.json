{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release",
        "simulator": false
      }
    },
    "apk-production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "EXPO_ANDROID_TARGET_SDK_VERSION": "35",
        "EXPO_ANDROID_COMPILE_SDK_VERSION": "35",
        "EXPO_ANDROID_MIN_SDK_VERSION": "24",
        "EXPO_ANDROID_KOTLIN_VERSION": "1.9.25"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "EXPO_ANDROID_TARGET_SDK_VERSION": "35",
        "EXPO_ANDROID_COMPILE_SDK_VERSION": "35",
        "EXPO_ANDROID_MIN_SDK_VERSION": "24",
        "EXPO_ANDROID_KOTLIN_VERSION": "1.9.25"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6757887033"
      }
    }
  }
}
