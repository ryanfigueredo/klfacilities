{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release",
        "simulator": false
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://www.klfacilities.com.br"
      }
    },
    "apk-production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "EXPO_ANDROID_TARGET_SDK_VERSION": "35",
        "EXPO_ANDROID_COMPILE_SDK_VERSION": "35",
        "EXPO_ANDROID_MIN_SDK_VERSION": "24",
        "EXPO_PUBLIC_API_URL": "https://www.klfacilities.com.br"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "EXPO_ANDROID_TARGET_SDK_VERSION": "35",
        "EXPO_ANDROID_COMPILE_SDK_VERSION": "35",
        "EXPO_ANDROID_MIN_SDK_VERSION": "24",
        "EXPO_PUBLIC_API_URL": "https://www.klfacilities.com.br"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "releaseStatus": "completed",
        "track": "production"
      },
      "ios": {}
    }
  }
}
